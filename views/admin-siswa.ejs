<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/admin-siswa.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js"></script>
  <title>Siswa</title>
</head>

<body>
  <div class="wrapper">
    <div class="sidebar">
        <ul>
            <li><a href="/admin">
                <span class="icon"> <i class="fas fa-security"></i></span>
                <span class="list"> PTMT </span>
              </a> </li>
            
            <li><a href="/admin-kelas">
                <span class="icon"> <i class="fas fa-security"></i></span>
                <span class="list"> Kelas </span>
            </a> </li>
    
            <li><a href="/admin-ruangan">
                <span class="icon"> <i class="fas fa-security"></i></span>
                <span class="list"> Ruangan </span>
              </a> </li>
    
            <li><a href="/admin-siswa">
                <span class="icon"> <i class="fas fa-security"></i></span>
                <span class="list"> Siswa </span>
              </a> </li>
    
            <li><a href="/admin-guru">
                <span class="icon"> <i class="fas fa-security"></i></span>
                <span class="list"> Guru </span>
              </a> </li>
    
          </ul>

      <div class="logosmart">

        <img src="/logosdsmart.png">
        <h1> <a href="/home.html">Log Out </a></h1>



    </div>
  </div>

  <div class="judul">
    <div class="biodata">
      <h1>
        Daftar Siswa
      </h1>
    </div>
  </div>

  <div class="tambah">
    <button type="button" onclick="location.href='/admin-importsiswa'">+ Import Data</button>
  </div>
  <div class="context">
    <div class="page">
      <%- include('./layout/pagination.ejs') %>
    </div>
    <table class="styled-table">
      <thead>
        <tr>
            <th>NISN</th>
            <th>Nama</th>
            <th>Kelas</th>
            <th>Username</th>
        </tr>
    </thead>
    <tbody>
        <% for(const desc of results) { %>
            <tr>
                <td><%= desc.NISN %></td>
                <td><%= desc.nama %></td>
                <td><%= desc.namaKelas %></td>
                <td><%= desc.username %></td>
            </tr>
        <% } %>
    </tbody>
    </table>
  </div>


</div>

</body>

</html>